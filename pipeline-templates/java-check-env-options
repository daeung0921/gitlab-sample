[root@ip-172-31-41-4 tmp]# cat EnvDump.java 
public class EnvDump {
    public static void main(String[] args) {
        System.getenv().forEach((k, v) -> {
            System.out.println(k + "=" + v);
        });
    }
}

[root@ip-172-31-41-4 tmp]# javac EnvDump.java
[root@ip-172-31-41-4 tmp]# java EnvDump | sort